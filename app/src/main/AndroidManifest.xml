<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ekzhu.musicstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PlayingActivity"
            android:label="@string/playing"/>
        <activity android:name=".SongsActivity"
            android:label="@string/songs"/>
        <activity android:name=".ArtistActivity"
            android:label="@string/artist"/>
        <activity android:name=".AlbumActivity"
            android:label="@string/album"/>
        <activity android:name=".PlaylistActivity"
            android:label="@string/playlist"></activity>
    </application>

</manifest>